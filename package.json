{
  "name": "server-fuqs",
  "version": "0.0.1",
  "description": "",
  "scripts": {
    "start": "nodemon ./src/index.js",
    "test": "nyc ava",
    "watch:test": "ava --watch",
    "lint": "eslint \"./src/**/*.+(js|jsx)\"",
    "lint:fix": "eslint \"./src/**/*.+(js|jsx)\" --fix",
    "format": "prettier --write \"src/**/*.+(js|jsx|json|css|md)\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RipDevil/server-FUQs.git"
  },
  "keywords": [
    "mock",
    "faq",
    "fuq",
    "fuqs",
    "server"
  ],
  "author": "MarkAnn",
  "license": "GPL-3.0-or-later",
  "dependencies": {
    "@koa/cors": "^3.1.0",
    "bcryptjs": "^2.4.3",
    "dotenv": "^8.2.0",
    "jsonwebtoken": "^8.5.1",
    "koa": "^2.13.0",
    "koa-bodyparser": "^4.3.0",
    "koa-jwt": "^4.0.0",
    "koa-router": "^9.1.0",
    "mongoose": "^5.9.27",
    "mongoose-private-paths": "^0.7.0",
    "node-banner": "^1.4.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "ava": "^3.15.0",
    "eslint": "^7.18.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "husky": "^4.3.8",
    "lint-staged": "^10.5.3",
    "mongodb-memory-server": "^6.9.3",
    "nodemon": "^2.0.4",
    "nyc": "^15.1.0",
    "prettier": "^2.2.1",
    "supertest-koa-agent": "^0.3.2"
  },
  "ava": {
    "files": [
      "tests/**/*.js"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.+(js|jsx)": [
      "eslint --fix",
      "git add"
    ],
    "*.+(json|css|md)": [
      "prettier --write",
      "git add"
    ]
  }
}
